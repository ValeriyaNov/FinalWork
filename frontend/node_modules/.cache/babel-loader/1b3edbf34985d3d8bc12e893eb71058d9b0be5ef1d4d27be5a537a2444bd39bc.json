    console.log(response.data)\r\n            // Опционально обновить интерфейс, чтобы удалить удалённый элемент\r\n            this.users = this.users.filter(item => item.id !== itemId);\r\n          })\r\n          .catch(error => {\r\n            console.error('Ошибка удаления элемента:', error);\r\n          });\r\n    },\r\n    submitForm(itemId){\r\n      //let selector = \"#\" + itemId\r\n      let array = document.getElementsByName(itemId);\r\n      //this.testUrl = parent;\r\n      //let array = parent.children;\r\n      this.testUser = array.children[1].value;\r\n      this.testUrl = array[1].children[1].children[1];\r\n      this.newUser.name = array[1].children[1].value;\r\n      this.newUser.password = array[2].children[1].value;\r\n      this.newUser.role = array[3].children[1].value;\r\n      this.newUser.id = itemId;\r\n      this.testUrl = '/users/' + itemId;\r\n      this.testUser = this.newUser;\r\n      AXIOS.put('/users/{$itemId}', this.newUser)\r\n          .then(response => {\r\n            console.log(response.data);\r\n            // дальнейшая обработка ответа\r\n            this.loadGreeting();\r\n          })\r\n          .catch(error => {\r\n            console.error(error);\r\n            // обработка ошибок\r\n          });\r\n      this.classVisibleForm = 'novisible';\r\n      this.classVisibleTable = 'visible';\r\n    },\r\n    loadGreeting() {\r\n      AXIOS.get('/users/all')\r\n          .then(response => {\r\n            this.$data.users = response.data;\r\n\r\n          })\r\n          .catch(error => {\r\n            console.log('ERROR: ' + error.response.data);\r\n          })\r\n\r\n    },\r\n    submitFormAdd(){\r\n      AXIOS.post('/users/user', this.newUserAdd)\r\n          .then(response => {\r\n            console.log(response.data);\r\n            // дальнейшая обработка ответа\r\n            this.loadGreeting();\r\n          })\r\n          .catch(error => {\r\n            console.error(error);\r\n            // обработ